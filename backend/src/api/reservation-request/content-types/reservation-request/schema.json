{
  "kind": "collectionType",
  "collectionName": "reservation_requests",
  "info": {
    "singularName": "reservation-request",
    "pluralName": "reservation-requests",
    "displayName": "Reservation Request"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "fullName": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "paymentMethod": {
      "type": "enumeration",
      "required": true,
      "enum": [
        "credit_card",
        "debit_card",
        "paypal",
        "apple_pay"
      ]
    },
    "termsAccepted": {
      "type": "boolean",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "required": true,
      "default": "pending",
      "enum": [
        "pending",
        "approved",
        "rejected"
      ]
    },
    "message": {
      "type": "text"
    },
    "container": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::container.container",
      "inversedBy": "reservation_requests"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "reservationRequests"
    }
  }
}
